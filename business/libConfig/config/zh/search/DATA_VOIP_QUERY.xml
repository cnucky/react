            <Condition Item="USER_NICKNAME,FRIEND_NICKNAME" DisplayName="用户昵称" ControlType="Text"/>
            <Condition Item="TIMEZONE,FRIEND_TIMEZONE" DisplayName="用户时区" ControlType="DropDownMultiSelect" ValueType="multi" ItemConfig="DIC_TIMEZONE"/>
        </ConditionGroup>
        <ConditionGroup GroupName="数据标签">
            <Condition Item="DATA_CHARACTER" DisplayName="数据特性" ControlType="DropDownMultiSelect" ValueType="multi" ItemConfig="DIC_DATA_CHARACTER"/>
